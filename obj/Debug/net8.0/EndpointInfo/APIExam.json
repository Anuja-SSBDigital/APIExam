                  },
                  "CollegeId": {
                    "type": "integer",
                    "format": "int32"
                  },
                  "FacultyId": {
                    "type": "integer",
                    "format": "int32"
                  },
                  "ExamTypeId": {
                    "type": "integer",
                    "format": "int32"
                  }
                }
              },
              "encoding": {
                "StudentIds": {
                  "style": "form"
                },
                "CollegeId": {
                  "style": "form"
                },
                "FacultyId": {
                  "style": "form"
                },
                "ExamTypeId": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
