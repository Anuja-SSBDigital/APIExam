          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "CollegeId": {
                    "type": "integer",
                    "format": "int32"
                  },
                  "StudentName": {
                    "type": "string"
                  },
                  "FacultyId": {
                    "type": "integer",
                    "format": "int32"
                  },
                  "ExamId": {
                    "type": "integer",
                    "format": "int32"
                  },
                  "CategoryName": {
                    "type": "string"
                  },
                  "SubCategory": {
                    "type": "string"
                  }
                }
              },
              "encoding": {
                "CollegeId": {
                  "style": "form"
                },
                "StudentName": {
                  "style": "form"
                },
                "FacultyId": {
                  "style": "form"
                },
                "ExamId": {
                  "style": "form"
                },
                "CategoryName": {
                  "style": "form"
